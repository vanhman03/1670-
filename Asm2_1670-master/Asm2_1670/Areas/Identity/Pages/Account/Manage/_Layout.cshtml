@{
    if (ViewData.TryGetValue("ParentLayout", out var parentLayout) && parentLayout !=  null)
    {
        Layout = parentLayout.ToString();
    }
    else
    {
        Layout = "/Views/Shared/_Layout.cshtml";
    }
}


    <div class="row">
        
            @RenderBody()
</div>

@section Scripts {
    @RenderSection("Scripts", required: false)
}
