@model Asm2_1670.Models.User
<body>
<div class="theme-layout" id="scrollup">
	<section class="overlape">
		<div class="block no-padding">
			<div data-velocity="-.1" style="background: url(images/mslider1.jpg) repeat scroll 50% 422.28px transparent;" class="parallax scrolly-invisible no-parallax"></div><!-- PARALLAX BACKGROUND IMAGE -->
			<div class="container fluid">
				<div class="row">
					<div class="col-lg-12">
						<div class="inner-header">
							<h3>Welcome Admin</h3>
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>

	<section>
		<div class="block no-padding">
			<div class="container">
				 <div class="row no-gape">
					 <div class="col-lg-9 column">
						 <div class="padding-left">
							 <div class="profile-title">
								 <h3>Reset password</h3>
							 </div>
							 <div class="profile-form-edit">
								 <form method="post">
									 <div class="row">
										 <div class="col-lg-12">
											 <span for="id" class="pf-title">ID</span>
											 <div class="pf-field">
												 <input id="id" name="id" value="@Model.Id" readonly>
											 </div>
										 </div>
										<div class="col-lg-12">
											 <span for="password" class="pf-title">New password</span>
											 <div class="pf-field">
												 <input id="pass" name="pass" type="text">
											 </div>
										 </div>
										 <div class="col-lg-12">
											 <button type="submit" class="btn btn-primary">Change</button>
											 <a asp-controller="Account" asp-action="Index" class="btn btn-primary">Back to List</a>
										 </div>
									 </div>
								 </form>
							 </div>
						 </div>
					</div>
				 </div>
			</div>
		</div>
	</section>
	</div>

<!-- Include Date Range Picker -->
<link rel="stylesheet" type="text/css" href="css/bootstrap-datepicker.css">
<script src="~/js/bootstrap-datepicker.js" type="text/javascript"></script>
<script>
		$(function(){
			$('.datepicker').datepicker({
				format: 'mm-dd-yyyy'
			});
		});
</script>
</body>